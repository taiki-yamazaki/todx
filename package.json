{
  "name": "todx",
  "version": "1.0.0",
  "description": "OTTOP hackason",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist out",
    "build": "webpack --display-error-details --progress --color --mode production",
    "webpack": "webpack --display-error-details --progress --color --mode development",
    "watch": "webpack --display-error-details --progress --color --watch --mode development",
    "dev": "webpack-dev-server --progress --colors --hot --content-base ./dist --mode development",
    "test": "jest"
  },
  "repository": "git+https://github.com/taiki-yamazaki/todx.git",
  "keywords": [],
  "author": "taiki-yamazaki<taiki.yamazaki@okinawaopenlabs.org>",
  "license": "Okinawa open laboratory",
  "bugs": {
    "url": "https://github.com/taiki-yamazaki/todx/issues"
  },
  "homepage": "https://github.com/taiki-yamazaki/todx#readme",
  "dependencies": {
    "@types/google-maps": "^3.2.1",
    "google-maps": "^3.3.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "copy-webpack-plugin": "^5.0.5",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/test/**/*.(test|spec).(ts|tsx|js|jsx)"
    ]
  }
